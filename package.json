{
  "name": "parcel-webapp-template",
  "version": "1.0.0",
  "description": "Boilerplate for a simple parcel based web app",
  "main": "index.js",
  "scripts": {
    "prestart": "rimraf build/*",
    "start": "parcel src/index.html -d build/",
    "prebuild": "rimraf dist/*",
    "build": "parcel build src/index.html --public-url ./",
    "watch": "parcel watch src/index.html -d build/",
    "lint": "eslint *.js src/**/*.js"
  },
  "author": "Santiago Pintus",
  "license": "GPL-2.0",
  "devDependencies": {
    "babel-core": "",
    "babel-eslint": "",
    "babel-preset-env": "",
    "eslint": "",
    "eslint-config-airbnb": "",
    "eslint-plugin-compat": "",
    "eslint-plugin-import": "",
    "eslint-plugin-jsx-a11y": "",
    "eslint-plugin-react": "",
    "eslint-plugin-react-hooks": "",
    "parcel-bundler": "",
    "rimraf": ""
  },
  "resolutions": {}
}
